{
  "name": "NeutronTrader",
  "version": "1.0.0",
  "description": "A simple trading bot for Binance using Electron and React",
  "main": "electron/main.js",
  "scripts": {
    "start": "react-app-rewired start",
    "react-start": "react-app-rewired start",
    "electron-start": "wait-on http://localhost:3000 && electron .",
    "dev": "concurrently \"npm run react-start\" \"npm run electron-start\"",
    "build": "react-app-rewired build",
    "electron-build": "electron-builder"
  },
  "dependencies": {
    "binance-api-node": "^0.11.47",
    "buffer": "^6.0.3",
    "electron-is-dev": "^2.0.0",
    "https-browserify": "^1.0.0",
    "node-schedule": "^2.1.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "recharts": "^2.1.8",
    "stream-http": "^3.2.0",
    "vm-browserify": "^1.1.2"
  },
  "devDependencies": {
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "concurrently": "^7.0.0",
    "crypto-browserify": "^3.12.1",
    "electron": "^36.2.0",
    "electron-builder": "^26.0.12",
    "net": "^1.0.2",
    "stream-browserify": "^3.0.0",
    "tls-browserify": "^0.2.2",
    "url": "^0.11.4",
    "wait-on": "^8.0.3"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "build": {
    "appId": "com.nublex.neutrontrader",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "electron/**/*",
      "node_modules/**/*"
    ],
    "linux": {
      "target": "AppImage"
    },
    "mac": {
      "category": "public.app-category.finance"
    },
    "productName": "NeutronTrader",
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "electron-is-dev": "^3.0.1",
    "node-schedule": "^2.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-scripts": "^0.0.0",
    "recharts": "3.2.1"
  },
  "description": "Professional trading platform built with Electron and React",
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "concurrently": "9.2.1",
    "cross-env": "^10.0.0",
    "electron": "38.1.2",
    "electron-builder": "26.0.20",
    "eslint": "9.36.0",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.4.0",
    "rimraf": "6.0.1",
    "wait-on": "9.0.0"
  },
  "homepage": "./",
  "main": "./main.js",
  "name": "neutron-trader",
  "scripts": {
    "build": "react-scripts build",
    "clean": "rimraf node_modules package-lock.json dist build",
    "dev": "concurrently \"npm:react\" \"wait-on http://localhost:3000 && npm:electron\"",
    "dist": "npm run build && electron-builder",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm:react\" \"wait-on http://localhost:3000 && npm:electron\"",
    "react": "react-scripts start",
    "start": "electron .",
    "test": "react-scripts test"
  },
  "version": "2.0.0"
}
